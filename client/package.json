{
  "name": "easynode-client",
  "version": "1.0.1",
  "description": "easynode-client",
  "bin": "./bin/www",
  "pkg": {
    "outputPath": "dist"
  },
  "scripts": {
    "client": "nodemon ./app/main.js",
    "pkgwin": "pkg . -t node16-win-x64",
    "pkglinux:x64": "pkg . -t node16-linux-x64 -o  dist/easynode-client-x64",
    "pkglinux:arm64": "pkg . -t node16-linux-arm64 -o dist/easynode-client-arm64"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "dependencies": {
    "axios": "0.27.2",
    "eslint": "8.56.0",
    "koa": "2.15.3",
    "node-os-utils": "1.3.7",
    "node-schedule": "2.1.1",
    "socket.io": "4.7.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "pkg": "5.8"
  }
}
